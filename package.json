{
  "name": "react_native_starter_cli",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "log:android": "react-native log-android",
    "link:fonts": "npx react-native-asset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint --max-warnings=0 .",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:start": "cd android && ./gradlew runRelease",
    "start": "react-native start",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-vector-icons/fontisto": "^12.3.0",
    "@react-native/new-app-screen": "0.80.1",
    "@react-navigation/drawer": "^7.5.3",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.2",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-reanimated": "^4.0.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-worklets": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.31.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "^0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.37.0"
  },
  "engines": {
    "node": ">=18"
  }
}
